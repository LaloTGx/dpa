<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="resultadoVM" type="com.dpa.app.modules.resultado.data.viewmodel.ResultadoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackfile"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameResultado"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyleamber_100">
            <LinearLayout android:id="@+id/linearColumnfile"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="start"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowfile"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageFile"
                            android:layout_height="@dimen/_52pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_width="@dimen/_51pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_file"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_file"/>
                        <ImageView android:id="@+id/imageFileOne"
                            android:layout_height="@dimen/_51pxh"
                            android:layout_marginStart="@dimen/_229pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_file_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_file_black_900"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowpreguntas"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_396pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPreguntas"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.28"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{resultadoVM.resultadoModel.txtPreguntas}"
                        style="@style/txtBioRhymeregular8" tools:text="@string/lbl_preguntas"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAuditivo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.23"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{resultadoVM.resultadoModel.txtAuditivo}"
                        style="@style/txtBioRhymeregular8" tools:text="@string/lbl_auditivo"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVisual"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_weight="0.18"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{resultadoVM.resultadoModel.txtVisual}"
                        style="@style/txtBioRhymeregular8" tools:text="@string/lbl_visual"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTacto"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.15"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{resultadoVM.resultadoModel.txtTacto}"
                        style="@style/txtBioRhymeregular8" tools:text="@string/lbl_tacto"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGusto"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.16"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{resultadoVM.resultadoModel.txtGusto}"
                        style="@style/txtBioRhymeregular8" tools:text="@string/lbl_gusto"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowpolygonone"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_42pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_179pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleblack_900_a5cornerRadius">
                    <FrameLayout android:id="@+id/frameStackpolygonone"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_58pxh">
                        <ImageView android:id="@+id/imagePolygonOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_56pxh"
                            android:layout_width="@dimen/_58pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_polygon1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_polygon1"/>
                        <ImageView android:id="@+id/imageNotification"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_35pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_notification"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                    </FrameLayout>
                    <FrameLayout android:id="@+id/frameStackpolygontwo"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginStart="@dimen/_90pxh"
                        android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_58pxh">
                        <ImageView android:id="@+id/imagePolygonTwo"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_56pxh"
                            android:layout_width="@dimen/_58pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_polygon1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_polygon1"/>
                        <FrameLayout android:id="@+id/frameStacklock"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_39pxv"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_7pxh"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_44pxh">
                            <ImageView android:id="@+id/imageLock"
                                android:layout_gravity="top|end"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="@dimen/_21pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_lock"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                            <ImageView android:id="@+id/imageUser"
                                android:layout_gravity="bottom|start"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="@dimen/_30pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_user"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        </FrameLayout>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackhactividadesbg"
                android:layout_gravity="center"
                android:layout_height="@dimen/_800pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_1pxh" android:layout_width="@dimen/_360pxh">
                <ImageView android:id="@+id/imageHActividadesBg"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_hactividadesbg}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_hactividadesbg"/>
                <FrameLayout android:id="@+id/frameStackansiedadalta"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_520pxv"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_331pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnansiedadalta"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyleyellow_200cornerRadius">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtAnsiedadalta"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_33pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{resultadoVM.resultadoModel.txtAnsiedadalta}"
                            style="@style/txtBioRhymebold26" tools:text="@string/lbl_ansiedad_alta"/>
                        <TextView android:gravity="center_horizontal"
                            android:id="@+id/txtDescription"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:text="@{resultadoVM.resultadoModel.txtDescription}"
                            style="@style/txtBioRhymeregular16" tools:text="@string/msg_miedo_p_nico"/>
                        <ImageView android:gravity="center_horizontal"
                            android:id="@+id/imageGrafansOne"
                            android:layout_height="@dimen/_122pxh"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_grafans1}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_grafans1"/>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLineSeven"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_29pxv" android:layout_width="@dimen/_288pxh"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowellipseseven"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_43pxv"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <FrameLayout
                                android:id="@+id/frameStackellipseseven"
                                android:layout_height="@dimen/_61pxv" android:layout_width="@dimen/_82pxh">
                                <View
                                    android:background="@drawable/rectangle_bg_bluegray_100_radius_41"
                                    android:id="@+id/viewEllipseSeven"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_61pxv" android:layout_width="@dimen/_82pxh"/>
                                <LinearLayout
                                    android:id="@+id/linearColumnellipsesix"
                                    android:layout_gravity="top|center"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_10pxv"
                                    android:layout_marginEnd="@dimen/_9pxh"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_width="match_parent" android:orientation="vertical">
                                    <View
                                    android:background="@drawable/rectangle_bg_pink_900_radius_31_5"
                                    android:id="@+id/viewEllipseSix"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_18pxv" android:layout_width="@dimen/_63pxh"/>
                                    <ImageView
                                    android:id="@+id/imageHome"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_27pxh"
                                    android:layout_marginEnd="@dimen/_19pxh"
                                    android:layout_marginStart="@dimen/_19pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="@dimen/_23pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_home_black_900"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_home_black_900"/>
                                </LinearLayout>
                            </FrameLayout>
                            <TextView
                                android:id="@+id/txtUltimaConsulta"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_6pxv"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_weight="0.32"
                                android:layout_width="0dp"
                                android:text="@{resultadoVM.resultadoModel.txtUltimaConsulta}"
                                style="@style/txtComicNeuebold12" tools:text="@string/msg_ultima_consulta"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnDescargar"
                                android:layout_height="@dimen/_41pxv"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_31pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="@dimen/_91pxh"
                                android:paddingBottom="@dimen/_14pxv"
                                android:paddingEnd="@dimen/_14pxh"
                                android:paddingStart="@dimen/_16pxh"
                                android:paddingTop="@dimen/_14pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_descargar"
                                android:textAllCaps="false"
                                style="@style/btnSolidRoundedOutline_10" tools:text="@string/lbl_descargar"/>
                        </LinearLayout>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnResultados"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_80pxh"
                        android:layout_marginStart="@dimen/_80pxh"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_11pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_resultados2"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_8" tools:text="@string/lbl_resultados2"/>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/deep_orange_100_cc"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_21pxv"
            android:layout_marginEnd="@dimen/_142pxh"
            android:layout_marginStart="@dimen/_142pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_70pxh"
            app:srcCompat="@drawable/img_home" app:tint="@color/black_900"/>
    </FrameLayout>
</layout>
