<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="visualVM" type="com.dpa.app.modules.visual.data.viewmodel.VisualVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackfile"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameVisual"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyleamber_100">
            <LinearLayout android:id="@+id/linearColumnfile"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowfile"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageFile"
                            android:layout_height="@dimen/_52pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_51pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_file"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_file"/>
                        <ImageView android:id="@+id/imageImageNine"
                            android:layout_height="@dimen/_65pxh"
                            android:layout_marginStart="@dimen/_212pxh"
                            android:layout_width="@dimen/_63pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_image30}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image30"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <View
                    android:background="@drawable/rectangle_bg_yellow_200_border_black_900_radius_10"
                    android:id="@+id/viewRectangleFortyEight"
                    android:layout_height="@dimen/_521pxh"
                    android:layout_marginTop="@dimen/_28pxv" android:layout_width="@dimen/_327pxh"/>
                <LinearLayout android:id="@+id/linearRowpolygonone"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleblack_900_a5cornerRadius">
                    <FrameLayout android:id="@+id/frameStackpolygonone"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_58pxh">
                        <ImageView android:id="@+id/imagePolygonOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_56pxh"
                            android:layout_width="@dimen/_58pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_polygon1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_polygon1"/>
                        <ImageView android:id="@+id/imageNotification"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_35pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_notification"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                    </FrameLayout>
                    <FrameLayout android:id="@+id/frameStackpolygontwo"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginStart="@dimen/_90pxh"
                        android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_58pxh">
                        <ImageView android:id="@+id/imagePolygonTwo"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_56pxh"
                            android:layout_width="@dimen/_58pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_polygon1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_polygon1"/>
                        <FrameLayout android:id="@+id/frameStacklock"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_39pxv"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_7pxh"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_44pxh">
                            <ImageView android:id="@+id/imageLock"
                                android:layout_gravity="top|end"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="@dimen/_21pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_lock"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                            <ImageView android:id="@+id/imageUser"
                                android:layout_gravity="bottom|start"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="@dimen/_30pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_user"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        </FrameLayout>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackhactividadesbg"
                android:layout_gravity="center"
                android:layout_height="@dimen/_800pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_1pxh" android:layout_width="@dimen/_360pxh">
                <ImageView android:id="@+id/imageHActividadesBg"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_hactividadesbg}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_hactividadesbg"/>
                <LinearLayout android:id="@+id/linearColumnvisualiza"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnVisualiza"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_60pxh"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_11pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_visualiza2"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_8" tools:text="@string/lbl_visualiza2"/>
                    <FrameLayout android:id="@+id/frameStackmostrar"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_456pxv"
                        android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_291pxh">
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerVisual"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.StaggeredGridLayoutManager"
                            app:spanCount="2"
                            itemSpace="@{@dimen/_15pxh}"
                            tools:itemCount="6" tools:listitem="@layout/row_visual"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnMostrarTwo"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_44pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_138pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_26pxh"
                            android:paddingStart="@dimen/_24pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_mostrar"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline_9" tools:text="@string/lbl_mostrar"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnMostrarThree"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_44pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_138pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_26pxh"
                            android:paddingStart="@dimen/_24pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_mostrar"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline_9" tools:text="@string/lbl_mostrar"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnMostrarFour"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_44pxv"
                            android:layout_marginBottom="@dimen/_154pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_154pxv"
                            android:layout_width="@dimen/_138pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_26pxh"
                            android:paddingStart="@dimen/_24pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_mostrar"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline_9" tools:text="@string/lbl_mostrar"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnMostrarFive"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_44pxv"
                            android:layout_marginBottom="@dimen/_90pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_90pxv"
                            android:layout_width="@dimen/_138pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_26pxh"
                            android:paddingStart="@dimen/_24pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_mostrar"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline_9" tools:text="@string/lbl_mostrar"/>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/deep_orange_100_cc"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_21pxv"
            android:layout_marginEnd="@dimen/_142pxh"
            android:layout_marginStart="@dimen/_142pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_70pxh"
            app:srcCompat="@drawable/img_home" app:tint="@color/black_900"/>
    </FrameLayout>
</layout>
