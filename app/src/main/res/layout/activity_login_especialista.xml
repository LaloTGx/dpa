<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginEspecialistaVM" type="com.dpa.app.modules.loginespecialista.data.viewmodel.LoginEspecialistaVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLoginespeciali"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackloginpacientebg"
            android:layout_gravity="center"
            android:layout_height="@dimen/_800pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_width="@dimen/_360pxh" style="@style/groupStyleamber_100">
            <ImageView android:id="@+id/imageLoginpacienteBg"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_loginpacientebg}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_loginpacientebg"/>
            <LinearLayout android:id="@+id/linearColumniniciar"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_104pxv"
                android:layout_marginEnd="@dimen/_115pxh"
                android:layout_marginStart="@dimen/_115pxh"
                android:layout_marginTop="@dimen/_104pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnIniciar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_iniciar"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_11" tools:text="@string/lbl_iniciar"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnRegistrarse"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_25pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_registrarse"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_11" tools:text="@string/lbl_registrarse"/>
            </LinearLayout>
        </FrameLayout>
        <ImageView android:id="@+id/imageDPAOne"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_331pxh"
            android:layout_marginBottom="@dimen/_212pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_212pxv"
            android:layout_width="@dimen/_360pxh"
            android:scaleType="fitXY" imageRes="@{@drawable/img_dpa1}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_dpa1"/>
    </FrameLayout>
</layout>
