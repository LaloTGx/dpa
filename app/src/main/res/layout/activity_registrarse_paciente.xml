<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registrarsePacienteVM" type="com.dpa.app.modules.registrarsepaciente.data.viewmodel.RegistrarsePacienteVM"/>
    </data>
    <FrameLayout android:id="@+id/frameRegistrarsepac"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylelight_green_301">
        <View android:background="@color/amber_100"
            android:id="@+id/viewRectangleOne"
            android:layout_gravity="center"
            android:layout_height="@dimen/_800pxh"
            android:layout_marginStart="@dimen/_1pxh" android:layout_width="@dimen/_360pxh"/>
        <FrameLayout android:id="@+id/frameStackhactividadesbg"
            android:layout_gravity="center"
            android:layout_height="@dimen/_800pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh" android:layout_width="@dimen/_360pxh">
            <ImageView android:id="@+id/imageHActividadesBg"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_hactividadesbg}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_hactividadesbg"/>
            <LinearLayout android:id="@+id/linearColumnlogoone"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_40pxh"
                android:layout_marginVertical="@dimen/_40pxv"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageLogoOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_94pxh"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_width="@dimen/_110pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logo1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logo1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNombre"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registrarsePacienteVM.registrarsePacienteModel.txtNombre}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_nombre"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_100_border_black_900_radius_23_5"
                    android:id="@+id/viewRectangleThree"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_47pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_233pxh"/>
                <FrameLayout
                    android:id="@+id/frameStackrectanglethreeOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_61pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_233pxh">
                    <View
                        android:background="@drawable/rectangle_bg_gray_100_border_black_900_radius_23_5"
                        android:id="@+id/viewRectangleThreeOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_47pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_233pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtApellido"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_93pxh"
                        android:layout_marginStart="@dimen/_93pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{registrarsePacienteVM.registrarsePacienteModel.txtApellido}"
                        style="@style/txtInterregular12" tools:text="@string/lbl_apellido"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCorreoElectron"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registrarsePacienteVM.registrarsePacienteModel.txtCorreoElectron}"
                    style="@style/txtInterregular12" tools:text="@string/msg_correo_electron"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_100_border_black_900_radius_23_5"
                    android:id="@+id/viewRectangleThreeTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_47pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_233pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContrasea"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registrarsePacienteVM.registrarsePacienteModel.txtContrasea}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_contrase_a"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_101_border_black_900_radius_23_5"
                    android:id="@+id/viewRectangleThreeThree"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_47pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_233pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNumeroTelefoni"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registrarsePacienteVM.registrarsePacienteModel.txtNumeroTelefoni}"
                    style="@style/txtInterregular12" tools:text="@string/msg_numero_telefoni"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_100_border_black_900_radius_23_5"
                    android:ellipsize="end"
                    android:hint="@string/lbl_52"
                    android:id="@+id/etGroupEightyEight"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={registrarsePacienteVM.registrarsePacienteModel.etGroupEightyEightValue}"
                    android:textColorHint="@color/black_900_9e"
                    style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEdad"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registrarsePacienteVM.registrarsePacienteModel.txtEdad}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_edad"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_100_border_black_900_radius_23_5"
                    android:id="@+id/viewRectangleThreeFour"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_47pxv"
                    android:layout_marginEnd="@dimen/_44pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_145pxh"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnIniciar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_61pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_iniciar"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_7" tools:text="@string/lbl_iniciar"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
