<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="iniciarSesionPacienteVM" type="com.dpa.app.modules.iniciarsesionpaciente.data.viewmodel.IniciarSesionPacienteVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearIniciarSesion"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleamber_100">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackloginpaciente"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageLoginpaciente"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_loginpaciente}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_loginpaciente"/>
            <LinearLayout android:id="@+id/linearColumnlogoone"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_104pxv"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_104pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageLogoOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_111pxh"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_149pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logo1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logo1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCorreoElectron"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{iniciarSesionPacienteVM.iniciarSesionPacienteModel.txtCorreoElectron}"
                    style="@style/txtInterregular12" tools:text="@string/msg_correo_electron"/>

                <EditText
                    android:id="@+id/editTextPaciEmailAddress_Access"
                    android:background="@drawable/rectangle_bg_gray_100_border_black_900_radius_23_5"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_47pxv"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_245pxh"
                    android:inputType="textEmailAddress" />

                <TextView android:ellipsize="end"
                    android:id="@+id/txtContrasea"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{iniciarSesionPacienteVM.iniciarSesionPacienteModel.txtContrasea}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_contrase_a"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_100_border_black_900_radius_23_5"
                    android:id="@+id/editTextPaciPassword_Access"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_47pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_244pxh"
                    android:ems="10"
                    android:inputType="textPassword"/>


                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnIniciar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_63pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_iniciar"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_7" tools:text="@string/lbl_iniciar"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnRegistrarse"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_25pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_registrarse"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_7" tools:text="@string/lbl_registrarse"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
